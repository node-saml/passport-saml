{
  "name": "passport-saml",
  "version": "1.3.5",
  "license": "MIT",
  "keywords": [
    "saml",
    "adfs",
    "sso",
    "shibboleth"
  ],
  "description": "SAML 2.0 authentication strategy for Passport",
  "author": {
    "name": "Henri Bergius",
    "email": "henri.bergius@iki.fi",
    "url": "http://bergie.iki.fi"
  },
  "contributors": [
    "Michael Bosworth",
    "Herbert Vojčík",
    "Peter Loer",
    "Mark Stosberg",
    "Chris Barth"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/node-saml/passport-saml.git"
  },
  "main": "./lib/passport-saml",
  "dependencies": {
    "debug": "^3.1.0",
    "passport-strategy": "*",
    "q": "^1.5.0",
    "xml-crypto": "^1.5.3",
    "xml-encryption": "1.2.1",
    "xml2js": "0.4.x",
    "xmlbuilder": "^11.0.0",
    "xmldom": "0.1.x"
  },
  "devDependencies": {
    "@types/node": "^14.14.5",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "body-parser": "^1.17.1",
    "eslint": "^6.8.0",
    "express": "^4.16.2",
    "mocha": "^6.0.2",
    "passport": "0.4.x",
    "request": "^2.83.0",
    "should": "*",
    "sinon": "^7.2.7",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "test": "npm run lint && mocha",
    "lint": "./node_modules/.bin/eslint --ext .ts src || true",
    "lint:fix": "./node_modules/.bin/eslint --ext .ts --fix src || true",
    "prepublishOnly": "tsc"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": false
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "no-undefined": "off",
      "no-unused-vars": "off",
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
